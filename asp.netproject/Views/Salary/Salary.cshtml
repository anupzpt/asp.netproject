@model List<asp.netproject.Models.employee_salary_details>

@{
    ViewBag.Title = "Salary";
}

<h2>Salary</h2>
<style>
    .table{
      border:1px solid black;

    }
</style>

<table class="table">
    <tr>
        <th>
            SN
        </th>
        <th>
            Employee Name
        </th>
        <th>
            Salary Amount
        </th>
        <th>
            Paid Date
        </th>
        <th>
            Action
        </th>
    </tr>
    @foreach (var data in Model)
    {
        <tr>
           
            <td>@(data.employee_id)  </td>
            <td>@(data.salary_paid)  </td>
            <td>@(data.paid_date)  </td>
            @*,name=data.name*@
            @*<td><a href="@Url.Action("Edit", new {id=data.id})">Edit</a></td>
            <td><a href="@Url.Action("Delete", new {id=data.id})">Delete</a></td>*@

        </tr>
    }

</table>

